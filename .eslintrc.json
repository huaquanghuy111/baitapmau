{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base", "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "consistent-return": "off",
        "spaced-comment": "off",
        "no-return-await": "off",
        "object-shorthand": "off",
        "no-shadow": "off"

        
    }
}
